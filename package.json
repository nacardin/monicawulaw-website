{
  "name": "monicawulaw-website",
  "version": "0.0.0",
  "license": "LicenseRef-LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/nacardin/monicawulaw-website.git"
  },
  "author": "Nick Cardin <nick@cardin.email>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint monicawulaw-website",
    "build:client-and-server-bundles": "./build-client-and-server-bundles.sh",
    "build:prerender": "yarn run build:client-and-server-bundles && yarn run compile:server && yarn run generate:prerender",
    "build:ssr": "yarn run build:client-and-server-bundles && yarn run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "cp src/language-select.html dist/browser/index.html && cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "deploy": "ts-node ghp-deploy.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nguniversal/common": "~7.1.0",
    "@nguniversal/express-engine": "~7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "~7.1.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "~6.4.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "bootstrap": "4.3.1",
    "codelyzer": "~4.5.0",
    "gh-pages": "^2.0.1",
    "http-server": "~0.10.0",
    "ts-loader": "~4.2.0",
    "ts-node": "^8.0.2",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
